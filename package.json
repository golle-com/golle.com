{
  "name": "real-debrid-mobile",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "local": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:test": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "login": "npx wrangler login --config cloudflare/wrangler.toml",
    "whoami": "npx wrangler whoami",
    "devw": "npx wrangler dev --config cloudflare/wrangler.toml",
    "deploy": "npm run deploy:preview",
    "deploy:preview": "npm run build && npx wrangler pages deploy dist --project-name rd-static --branch master",
    "deploy:prod": "npm run build && npx wrangler pages deploy dist --project-name rd-static --branch main",
    "deploy:worker": "npx wrangler deploy --config cloudflare/wrangler.toml",
    "tail": "npx wrangler tail --config cloudflare/wrangler.toml",
    "test:time": "curl -sS https://rd-proxy.golle.workers.dev/time",
    "test:upstream": "curl -sS \"https://rd-proxy.golle.workers.dev/rest/1.0/time\""
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.1",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.13.0",
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^10.0.2",
    "eslint-plugin-react-refresh": "^0.5.2",
    "globals": "^17.3.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^7.3.1"
  }
}
